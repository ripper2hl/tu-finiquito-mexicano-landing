<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.html_pages %}
    {% if page.url != "/404.html" and page.title != "404" %}
  <url>
    <loc>{{ page.url | absolute_url }}</loc>
  </url>
    {% endif %}
  {% endfor %}
</urlset>
